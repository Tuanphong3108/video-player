{
  "name": "Video Player",
  "short_name": "Video player",
  "start_url": "index.html",
  "display": "standalone",
  "background_color": "#1c1b1f",
  "theme_color": "#1c1b1f",
  "icons": [
    {
      "src": "data:image/svg+xml;base64,",
      "sizes": "192x192",
      "type": "image/svg+xml",
      "purpose": "any maskable"
    },
    {
      "src": "data:image/svg+xml;base64,",
      "sizes": "512x512",
      "type": "image/svg+xml",
      "purpose": "any maskable"
    }
  ],
  "file_handlers": [
  {
    "action": "/open-file",
    "accept": {
      "video/*": [
        ".mp4",
        ".webm",
        ".mkv",
        ".mov",
        ".avi"
      ]
    }
  }
]
